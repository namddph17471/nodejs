{
    "version": 2,
    "builds": [
        {
            "src": "/src/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/products/(.*)",
            "dest": "/src/app.js"
        },
        {
            "src": "/api/users(.*)",
            "dest": "/src/app./js"
        },
        {
            "src": "/api/cateNew/(.*)",
            "dest": "/src/app.js"
        },
        {
            "src": "/api/cateProduct/(.*)",
            "dest": "/src/app.js"
        },
        {
            "src": "/api/news/(.*)",
            "dest": "/src/app.js"
        }
    ]
}